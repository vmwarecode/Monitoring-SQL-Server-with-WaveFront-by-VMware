              "name": "Buffer cache hit ratio",
              "sources": [
                {
                  "name": "Query",
                  "query": "ts(\"Buffer-cache-hit-ratio---Buffer-Manager\", source=\"controlcenter-lg\")",
                  "disabled": null,
                  "scatterPlotSource": "Y",
                  "querybuilderSerialization": "{\"_v\":1,\"metric\":\"ts(\\\"Buffer-cache-hit-ratio---Buffer-Manager\\\")\",\"filters\":[[[\"source\",\"controlcenter-lg\"]],\"or\"],\"functions\":[]}",
